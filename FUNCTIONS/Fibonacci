static void fib_sequence(int n) {
int n1=0;
  int n2=1;
  if(n==0){
    System.out.println(n1);
    return;
  }
  else if(n==1){
    System.out.println(n1);
    return;
  }
  else{
  System.out.println(n1);
  System.out.println(n2);
  for(int i=2;i<n;i++){
    int n3=n1+n2;
    System.out.println(n3);
    n1=n2;
    n2=n3;
   }
  }
}
